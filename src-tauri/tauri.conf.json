{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "AGNX Drawer",
  "version": "0.1.0",
  "identifier": "com.agnx.drawer",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3001",
    "beforeDevCommand": "",
    "beforeBuildCommand": "bash scripts/tauri-build.sh"
  },
  "app": {
    "windows": [
      {
        "title": "AGNX Drawer",
        "width": 1400,
        "height": 900,
        "resizable": true,
        "fullscreen": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
